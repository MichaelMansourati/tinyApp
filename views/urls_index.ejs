<html lang="en">
<head>
  <title>TinyApp</title>
</head>
<body>

    <% Object.keys(urls).forEach(function(key) {%>
      <span>localhost:8080/u/<%=key%></span>
      <span> <---> </span>
      <span><%= urls[key] %></span>
      <a href="localhost:8080/urls/<%+key%>">Edit</a>
      <form method="POST" action="/urls/<%= key %>/delete"  style="display: inline-block">
        <button>delete</button>
      </form>
      </br>
    <% }) %>

</body>
</html>


