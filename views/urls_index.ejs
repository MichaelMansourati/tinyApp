<html lang="en">
<head>
</head>
<body>
    <header>
      <% include partials/_header %>
    </header>
    <% Object.keys(urls).forEach(function(key) {%>
      <span>localhost:8080/u/<%=key%></span>
      <span> <---> </span>
      <span><%= urls[key] %></span>
      <span><a href="http://localhost:8080/urls/<%=key%>">Edit</a></span>
      <form method="POST" action="/urls/<%= key %>/delete"  style="display: inline-block">
        <button>delete</button>
      </form>
      </br>
    <% }) %>

</body>
</html>


